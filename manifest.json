{
  "manifest_version": 3,
  "name": "vote plugin",
  "version": "1.0.0",
  "description": "vote in plugin",
  "icons": {
    "16": "img/logo.png",
    "48": "img/logo.png",
    "128": "img/logo.png"
  },
  "background": {
    "service_worker": "js/background.js"
  },
  "action": {
    "default_icon": {
      "19": "img/logo.png",
      "38": "img/logo.png"
    },
    "default_title": "vote in plugin",
    "default_popup": "popup.html"
  },
  "permissions": [
    "contextMenus",

    "notifications",
    "webRequest",
    "storage",
    "tabs",
    "cookies",
    "http://*/*",
    "https://*/*",
    "scripting",
    "activeTab"
  ],
  "host_permissions": ["http://*/*", "https://*/*"],
  "homepage_url": "https://pr.dianguanweilai.net/",
  "devtools_page": "devtools.html"
}
